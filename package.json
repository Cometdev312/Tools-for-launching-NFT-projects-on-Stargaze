{
  "name": "stargaze-nft",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@cosmjs/cli": "^0.27.1"
  },
  "dependencies": {
    "ipfs-car": "^0.6.1"
  },
  "scripts": {
    "pack-images": "ipfs-car --pack images --output images.car",
    "pack-metadata": "ipfs-car --pack metadata --output metadata.car",
